{
  "name": "fastify-starter",
  "version": "0.4.0",
  "description": "Fastify Typescript Starter",
  "keywords": [
    "fastify",
    "fastify-starter"
  ],
  "license": "MIT",
  "author": "Huynh Duc Dung",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsup",
    "build:ts": "tsup --env.NODE_ENV production",
    "compile-schemas": "json2ts -i src/schemas -o src/types",
    "copy:assets": "cpy 'src/generated/*.json' dist/generated",
    "dev": "yarn compile-schemas && yarn dev:watch",
    "dev:watch": "tsx watch src/server.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "start": "tsx src/server.ts",
    "start:prod": "HYPERDX_API_KEY='bb2f34d0-55eb-4623-94e8-087962d2cc67' OTEL_SERVICE_NAME='fastify-starter' npx opentelemetry-instrument dist/server.js",
    "test": "cross-env TS_NODE_FILES=true tap --ts test/**/*.test.ts",
    "typecheck": "tsc -b"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@fastify/autoload": "5.7.1",
    "@fastify/cors": "8.4.0",
    "@fastify/env": "4.2.0",
    "@fastify/etag": "4.2.0",
    "@fastify/helmet": "11.1.1",
    "@fastify/multipart": "8.0.0",
    "@fastify/rate-limit": "8.0.3",
    "@fastify/sensible": "5.3.0",
    "@fastify/swagger": "8.11.0",
    "@fastify/swagger-ui": "1.10.0",
    "@hyperdx/node-opentelemetry": "0.3.3",
    "@opentelemetry/api": "1.6.0",
    "@opentelemetry/auto-instrumentations-node": "0.39.3",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.44.0",
    "@opentelemetry/exporter-trace-otlp-proto": "0.44.0",
    "@opentelemetry/sdk-metrics": "1.17.1",
    "@opentelemetry/sdk-node": "0.44.0",
    "@sinclair/typebox": "0.31.17",
    "altair-fastify-plugin": "5.2.5",
    "close-with-grace": "1.2.0",
    "dotenv": "16.3.1",
    "fastify": "4.24.1",
    "fastify-cli": "5.8.0",
    "fastify-plugin": "4.5.1",
    "fastify-tsconfig": "2.0.0",
    "graphql": "16.8.1",
    "mercurius": "13.1.0",
    "mercurius-codegen": "5.0.3",
    "mercurius-integration-testing": "8.1.1",
    "nexus": "1.3.0",
    "swagger-jsdoc": "6.2.8"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/busboy": "1.5.1",
    "@types/dotenv": "8.2.0",
    "@types/node": "20.8.5",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/tap": "15.0.9",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "cpy-cli": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.51.0",
    "eslint-config-productsway": "1.3.0",
    "fast-json-stringify": "5.8.0",
    "husky": "8.0.3",
    "json-schema-to-typescript": "13.1.1",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "sort-package-json": "2.6.0",
    "tap": "16.3.9",
    "ts-node": "10.9.1",
    "tsup": "7.2.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "cjs",
      "esm"
    ],
    "legacyOutput": true,
    "target": "es2018",
    "onSuccess": "npm run copy:assets"
  }
}
